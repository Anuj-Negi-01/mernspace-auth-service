{
   "name": "auth-service",
   "version": "1.0.0",
   "description": "User management service",
   "main": "index.js",
   "scripts": {
      "dev": "cross-env NODE_ENV=dev nodemon src/server.ts",
      "format:fix": "prettier . --write",
      "format:check": "prettier . --check",
      "lint:check": "eslint .",
      "lint:fix": "eslint . --fix",
      "test": "jest --watch --runInBand",
      "prepare": "husky",
      "start": "ts-node src/index.ts",
      "migration:generate": "typeorm-ts-node-commonjs -d src/config/data-source.ts migration:generate",
      "migration:run": "typeorm-ts-node-commonjs -d src/config/data-source.ts migration:run",
      "migration:create": "typeorm-ts-node-commonjs -d src/config/data-source.ts migration:create"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/Anuj-Negi-01/mernspace-auth-service.git"
   },
   "author": "Anuj Negi",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/Anuj-Negi-01/mernspace-auth-service/issues"
   },
   "homepage": "https://github.com/Anuj-Negi-01/mernspace-auth-service#readme",
   "devDependencies": {
      "@eslint/js": "^9.24.0",
      "@types/bcrypt": "^5.0.2",
      "@types/cookie-parser": "^1.4.8",
      "@types/express": "^5.0.1",
      "@types/jest": "^29.5.14",
      "@types/jsonwebtoken": "^9.0.9",
      "@types/node": "^22.14.1",
      "@types/supertest": "^6.0.3",
      "@types/winston": "^2.4.4",
      "cross-env": "^7.0.3",
      "eslint": "^9.24.0",
      "globals": "^16.0.0",
      "husky": "^9.1.7",
      "jest": "^29.7.0",
      "lint-staged": "^15.5.1",
      "mock-jwks": "^1.0.10",
      "nodemon": "^3.1.10",
      "prettier": "3.5.3",
      "supertest": "^7.1.0",
      "ts-jest": "^29.3.2",
      "ts-node": "^10.9.2",
      "typescript": "^5.8.3",
      "typescript-eslint": "^8.30.1"
   },
   "lint-staged": {
      "*.ts": [
         "npm run format:fix",
         "npm run lint:fix"
      ]
   },
   "dependencies": {
      "bcrypt": "^5.1.1",
      "cookie-parser": "^1.4.7",
      "dotenv": "^16.5.0",
      "express": "^5.1.0",
      "express-jwt": "^8.5.1",
      "express-validator": "^7.2.1",
      "http-errors": "^2.0.0",
      "jsonwebtoken": "^9.0.2",
      "jwks-rsa": "^3.2.0",
      "pg": "^8.14.1",
      "reflect": "^0.1.3",
      "reflect-metadata": "^0.2.2",
      "rsa-pem-to-jwk": "^1.1.3",
      "typeorm": "0.3.22",
      "winston": "^3.17.0"
   }
}